{
  "name": "wdio-browserstack-spec-reporter",
  "version": "1.0.0",
  "description": "A WebdriverIO plugin to report in spec style suited for BrowserStack",
  "author": "Timotej Zatko <timi.zatko@gmail.com>",
  "homepage": "https://github.com/timzatko/wdio-browserstack-spec-reporter",
  "license": "MIT",
  "main": "./build/index",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "build": "npx npm-run-all clean compile",
    "clean": "npx rimraf ./build",
    "compile": "babel src/ -d build/",
    "test": "run-s test:*",
    "test:eslint": "eslint src test",
    "test:unit": "jest",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/timzatko/wdio-browserstack-spec-reporter.git"
  },
  "keywords": [
    "webdriver",
    "wdio",
    "wdio-reporter"
  ],
  "bugs": {
    "url": "https://github.com/timzatko/wdio-browserstack-spec-reporter/issues"
  },
  "dependencies": {
    "@wdio/reporter": "6.3.6",
    "chalk": "^4.0.0",
    "easy-table": "^1.1.1",
    "pretty-ms": "^7.0.0"
  },
  "peerDependencies": {
    "@wdio/cli": "^6.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-function-bind": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "babel-cli": "^6.26.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
